{
  "name": "playwright-practice-events",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:debug": "playwright test --debug",
    "allure:generate": "allure generate ./allure-results --clean -o ./allure-report",
    "allure:open": "allure open ./allure-report",
    "test:allure": "npm run test && npm run allure:generate && npm run allure:open",
    "test:sanity": "npx playwright test \"@sanity\"",
    "test:regression": "npx playwright test \"@regression\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/node": "^25.0.3",
    "allure-commandline": "^2.36.0",
    "allure-playwright": "^3.4.3"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "path": "^0.12.7"
  }
}
